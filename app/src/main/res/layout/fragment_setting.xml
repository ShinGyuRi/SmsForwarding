<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="setting"
            type="com.abercompany.smsforwarding.fragment.SettingFragment"/>
    </data>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragment.SettingFragment"
        android:background="@color/white">

        <Button
            android:id="@+id/btn_add_last_rent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@color/black"
            android:onClick="@{setting.onClick}"
            android:text="마지막 월세 추가"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_register"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@color/black"
            android:text="문자 전화번호 등록"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_add_last_rent"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="@{setting.onClick}"/>

        <Button
            android:id="@+id/btn_check_defaulter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@color/black"
            android:text="미납자 조회"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_register"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="@{setting.onClick}" />

        <Button
            android:id="@+id/btn_check_leave_room"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@color/black"
            android:text="퇴실 임박자 조회"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="@+id/btn_add_last_rent"
            app:layout_constraintTop_toBottomOf="@+id/btn_add_last_rent"
            android:onClick="@{setting.onClick}" />

        <Button
            android:id="@+id/btn_check_elec_defaulter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@color/black"
            android:text="전기 미납자 조회"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_check_leave_room"
            app:layout_constraintTop_toBottomOf="@+id/btn_register"
            android:onClick="@{setting.onClick}" />

        <Button
            android:id="@+id/btn_register_etc_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@color/black"
            android:text="그외번호 등록"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_check_elec_defaulter"
            app:layout_constraintTop_toBottomOf="@+id/btn_check_defaulter"
            android:onClick="@{setting.onClick}"/>

        <Button
            android:id="@+id/btn_refresh_contacts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@color/black"
            android:text="입주자 전화번호 갱신"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="@+id/btn_check_leave_room"
            app:layout_constraintTop_toBottomOf="@+id/btn_check_leave_room"
            android:onClick="@{setting.onClick}"/>

        <Button
            android:id="@+id/btn_refresh_contacts_realty"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:background="@color/black"
            android:text="부동산 전화번호 갱신"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_refresh_contacts"
            app:layout_constraintTop_toTopOf="@+id/btn_refresh_contacts"
            android:onClick="@{setting.onClick}" />

        <Button
            android:id="@+id/btn_manage_elec"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@color/black"
            android:onClick="@{setting.onClick}"
            android:text="지하 전기 관리"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="@+id/btn_refresh_contacts"
            app:layout_constraintTop_toBottomOf="@+id/btn_refresh_contacts" />

        <Button
            android:id="@+id/btn_add_manage_building_contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:background="@color/black"
            android:onClick="@{setting.onClick}"
            android:text="건물관리 연락처"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_manage_elec"
            app:layout_constraintTop_toTopOf="@+id/btn_manage_elec" />

        <Button
            android:id="@+id/btn_select_leave_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:background="@color/black"
            android:text="퇴실임박자 날짜지정"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/btn_add_manage_building_contact"
            app:layout_constraintTop_toTopOf="@+id/btn_add_manage_building_contact"
            android:onClick="@{setting.onClick}"/>

    </android.support.constraint.ConstraintLayout>
</layout>